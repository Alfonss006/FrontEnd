{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "DataGroup": {
    "name" : "external" ,
    "urls": ["https://hacker-news.firebaseio.com/v0/item/**",
       "https://hacker-news.firebaseio.com/**"] ,
    "version?": 1 ,
    "cacheConfig": {
      "maxSize": 10,
      "maxAge": "1d" ,
      "timeout?": "4s",
      "strategy?": "freshness" 
    }
  }
}
